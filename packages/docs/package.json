{
  "name": "cowtown-docs",
  "private": true,
  "description": "cowtown documentation",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf .docz",
    "build": "run-s clean build:docz",
    "build:docz": "docz build",
    "start": "run-s clean watch:docs",
    "watch:docz": "docz dev",
    "watch:docs": "run-p watch:docz",
    "format": "prettier-standard --write \"src/**/*.js\"",
    "test": "echo 'no tests'"
  },
  "devDependencies": {
    "docz": "next",
    "docz-theme-default": "next",
    "surge": "^0.20.3",
    "prettier-standard": "^9.1.1",
    "typescript": "^3.1.6",
    "npm-run-all": "latest"
  }
}
