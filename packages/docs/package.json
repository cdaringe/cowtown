{
  "name": "cowtown-docs",
  "private": true,
  "description": "cowtown documentation",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "build:docz": "docz build",
    "build:modulified-demo": "ts-node scripts/modulify-demos.ts --build",
    "build": "run-s clean compile build:modulified-demo build:docz",
    "compile": "tsc -p tsconfig.jsextractor.json",
    "compile:watch": "tsc -p tsconfig.jsextractor.json -w",
    "clean": "rm -rf .docz && del 'src/**/*.{js,json}'",
    "start": "run-s clean watch:docs",
    "test": "echo 'add a test file for each little app'",
    "watch:modulified-demo": "ts-node scripts/modulify-demos.ts --watch",
    "watch:docs": "run-p compile:watch watch:modulified-demo watch:docz",
    "watch:docz": "docz dev"
  },
